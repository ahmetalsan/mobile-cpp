hello_world = interface +c {
	static create(): hello_world;
	get_hello_world(): string;
}

handshake_result_record = record {
	responseStatus: string;
}

api_handshake = interface +c {
	static create(): api_handshake;
	handshake(udId: string, appVersion: string, osVersion: string, osType: string): handshake_result_record;
}